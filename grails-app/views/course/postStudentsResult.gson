import edu.oswego.cs.lakerpolling.domains.AuthToken

response.status OK.value()
model {
    AuthToken token
    Integer courseID
    Collection<User> students = []
}

json {
    status "success"
    data {
        accessToken token.accessToken
        course_id courseID
        user g.render(template: "../template/students", model: [students:students])
    }
}